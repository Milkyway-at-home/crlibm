for i from 1 to 7 do
  printendian(fd,midI[i],isbig);
od:
lastx:=ieee2Hexa(midI[8]):
fprintf(<Font opaque="false">fd,"{{0x%+0.8s,0x%+0.8s}} /* %+0.10f */\n};\n\n", lastx[2-isbig], lastx[isbig+1], midI[8]):</Font>

#Write poly_log_fast_b 
#Assume all polynomial are of same degree

deg:=degree(Plist[1]):
