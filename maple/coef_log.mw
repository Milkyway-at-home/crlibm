for k from 1 to 7 do<Font italic="false" opaque="false" size="12" underline="false">
  fprintf(fd," /* polynomial %d */\n{\n",k);</Font>
  for j from 0 to 12 do
    coef:=coeff(Plist[k],x,j):
    printendian(fd,coef,isbig):
  od:
  inter:=nearest(<Font opaque="false">coeff(Plist[k],x,13)):</Font>
  coef:=ieee2Hexa(inter<Font opaque="false">):</Font>
  fprintf(fd,"{{0x%+0.8s,0x%+0.8s}} /* %+0.10f */\n},\n", coef[2-isbig], coef[isbig+1], inter):
od: <Font italic="false" opaque="false" size="12" underline="false">
fprintf(fd," /* polynomial %d */\n{\n",8);</Font><Font opaque="false">
  for j from 0 to 12 do
    coef:=coeff(Plist[8],x,j):
    printendian(fd,coef,isbig):
  od:
  inter:=nearest(coeff(Plist[k],x,13)):
  coef:=ieee2Hexa(inter):
  fprintf(fd,"{{0x%+0.8s,0x%+0.8s}} /* %+0.10f */\n}\n};\n\n", coef[2-isbig], coef[isbig+1], inter):</Font>

#Write poly_log_fast_l

fprintf(fd,"db_number const poly_log_fast_l[%d][%d] =\n{\n",npol,n);
for k from 1 to 7 do
  <Font italic="false" opaque="false" size="12" underline="false">fprintf(fd," /* polynomial %d */\n{\n",k);
  coef:=hi_lo(coeff(Plist[k],x,0)):</Font>
