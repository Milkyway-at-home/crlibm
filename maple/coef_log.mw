  fprintf(fd,"{{0x%+0.8s,0x%+0.8s}} /* %+0.10f */\n},\n", coef[2-isbig], coef[isbig+1], inter[2]):</Font>
od:<Font italic="false" opaque="false" size="12" underline="false">
