</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">taberr[5,1]+ infnorm((1-(expand(Plist[5]))/(ln(mid[5]+x))), x=-2**(-4)..2**(-4)); </Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output"></Equation></Text-field></Output></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input"/></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">logparameter:=proc(Plist,Ilist, err)
local tepsapprox, epsapprox, epsround, tepsround, k, delta, e, i, xmax, midI, tabe;

tepsapprox:=[]:  
tepsround:=[]:
tabe:=[]:
for i from 1 to 8 do
  xmax:=(Ilist[i,2]-Ilist[i,1])/2:
  midI:= Ilist[i,2]-xmax:

  epsapprox:= infnorm((1-(expand(Plist[i]))/(ln(midI+x))), x=-xmax..xmax);
  tepsapprox:=[op(tepsapprox),epsapprox]:

  epsround:= err[i,2]:
  tepsround:=[op(tepsround),epsround]:

  delta:= epsapprox + err[i,1] + 2**(-100):
  k:=abs(trunc(log(delta)/log(2.))+53):
  e:=evalf(1+((2**((54+k)*delta)/((2**(k)-1)*(1-2**(-53)))))):
 Digits:=11: 
 tabe:=[op(tabe), evalf(e)];
 Digits:=150:
od;
tabe;
end proc;</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output"></Equation></Text-field></Output></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input"><Font italic="false" size="12" underline="false">Plist:=[P0, P1, P2, P3, P4, P5, P6, P7]:
