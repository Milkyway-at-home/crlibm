Digits := 200:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle33" prompt="&gt; " style="_pstyle33">with (numapprox):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle33" prompt="&gt; " style="_pstyle33">? save</Text-field></Input></Group><Section collapsed="true"><Title><Text-field layout="_pstyle34" style="_pstyle34"><Font encoding="ISO8859-1">Proc\351dures utiles</Font></Text-field></Title><Group><Input><Text-field layout="_pstyle35" style="Text">Renvoie l'arrondi double IEEE au plus pres  de x (Jean-Michel):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">nearest := proc(u)
local arrondi, signe, x, exposant, mantisseinfinie, mantisse;
Digits:=200:
if u = 0 then arrondi := 0
else 
  if (u &lt; 0) then signe := -1; x := -u else signe := 1; x := u fi;
  exposant := floor(evalf(log(x)/log(2.0)));
  mantisseinfinie := x*2^(52-exposant);
  if frac(mantisseinfinie) &lt;&gt; 0.5 then mantisse := round(mantisseinfinie)
   else
     mantisse := floor(mantisseinfinie);
     if type(mantisse,odd) then mantisse := mantisse+1 fi;
   fi;
   arrondi := signe*mantisse*2^(exposant-52)
fi;
arrondi;
end:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle35" style="Text">Convertit un nombre au format double IEEE. </Text-field><Text-field layout="_pstyle35" style="Text">renvoie Exposant, fraction de la mantisse, mantisse en binaire.</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">IEEEdouble:=proc(x) local signe, logabsx, exposant, mantisse, mantisseinfinie, resultat; 
 Digits:=200;
if (x=0) then [0,0,0]; 
else 
 if (x&lt;0) then signe:=-1:
 else signe:=1:
 fi:
 exposant := floor(evalf(log(signe*x)/log(2.0)));
 if (exposant&gt;1023) then mantisse:=infinity:
 elif (exposant&lt;-1022) then mantisse:=0:
 else 
  mantisseinfinie := signe*x*2^(52-exposant);
  if frac(mantisseinfinie) &lt;&gt; 0.5 then mantisse := round(mantisseinfinie)
   else
     mantisse := floor(mantisseinfinie);
     if type(mantisse,odd) then mantisse := mantisse+1 fi;
   fi;
 mantisse:= signe*mantisse*2**(-52);
 Digits := 53;
 resultat:=convert(mantisse*2.0^(exposant),binary);
 fi;
[exposant,mantisse, resultat];
fi;
end:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">Digits:=400;IEEEdoubleSAVE(2.55397551016965318471108048470341600477695465087891e-1);
IEEEdouble(2.55397551016965318471108048470341600477695465087891e-1);
Digits;

</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM+SSdEaWdpdHNHNiIiJCsl</Equation></Text-field><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiMtSS9JRUVFZG91YmxlU0FWRUc2IjYjJCJUIip5M2xhcHgvZ1QuWlshMzZaPWAncCxedlJiIyEjXg==</Equation></Text-field></Output><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM3JSEiIyMiMXBgT2lLJDNnJSIxJ1xxdGkqZi5YJCJWKzU2NTU1NSs1LCssKzU2NjU2NSwrNSwsKysiISNh</Equation></Text-field><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiMiJCsl</Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1">Renvoie le polyn\364me tronqu\351 dont les coefficients sont des doubles IEEE.</Font></Text-field></Input></Group><Group><Input><Text-field layout="_pstyle38" prompt="&gt; " style="_cstyle496">poly_exact:=proc(P)
local deg,i, coef, coef_t:
global Q:
Q:= 0:
convert(Q, polynom):
deg:=degree(P,x):
  for i from 0 to deg do
    coef:=coeff(P,x,i):
    if ( coef=0 ) then;
    else 
      coef_t:=IEEEdouble(coef):
    Q:= Q+coef_t[2]*2**(coef_t[1])*x^i:
    fi:
  od:
return(evalf(Q));
end:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle39" style="_pstyle39"/><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1">Renvoie deux doubles IEEE correspondants a la partie haute et la partie basse de l'entr\351e x.</Font></Text-field><Text-field layout="_pstyle39" style="_pstyle39"/></Input></Group><Group><Input><Text-field layout="_pstyle38" prompt="&gt; " style="_cstyle496">hi_lo:= proc(x)
global x_hi, x_lo, res:
local tamp:
x_hi:= nearest(evalf(x)):
res:=x-x_hi:
if (res = 0) then
  x_lo:=0:
else
  x_lo:=nearest(evalf(res)):
end if;
(x_hi,x_lo);
end:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">a:=hi_lo(3.125420001245810000124545787575);
a[1];</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM+SSJhRzYiNiQjIjFqTVx3LCN5LigiMVtfbzgpKnpeQSMhMSY9Q09dazNZKSJBSz9kLVgqeSUzTS50Pz5bY1M=</Equation></Text-field><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiMjIjFqTVx3LCN5LigiMVtfbzgpKnpeQQ==</Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle39" style="_pstyle39"/><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1">Renvoie le polynome tronqu\351 associ\351 a P, dont les coefficients sont stock\351s sur deux doubles IEEE.</Font></Text-field><Text-field layout="_pstyle39" style="_pstyle39"/></Input></Group><Group><Input><Text-field layout="_pstyle38" prompt="&gt; " style="_cstyle496">poly_exact2:=proc(P,n)
local i, coef, coef_t:
global deg, Q, psup, pinf, pfull:
psup:=0: pinf:=0:
Q:=[];
convert(psup, polynom): convert(pinf, polynom):
deg:=degree(P,x):
  for i from 0 to deg do
    coef:=coeff(P,x,i):
    if (coef=0) then
      Q:= (Q,[0,0]):
    elif(coef=1) then
        Q:= (Q,[1,0]):
        psup:=psup+x^i:
    else        
      coef_t:=hi_lo(coef):
      Q:= (Q,[coef_t]):
      psup:=psup + coef_t[1]*x^i:
        if(i&lt;n) then
        pinf:=pinf + coef_t[2]*x^i:
        fi;
    end if;
  od:
Q:=Q[2..deg+2];
pfull:=expand(psup+pinf):
end:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle39" style="_pstyle39"/><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1">Renvoie l'\351criture hexadecimale du nombre double IEEE associ\351 a l'entree x</Font></Text-field><Text-field layout="_pstyle39" style="_pstyle39"/></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">Hexa:= proc(x)
  local unite;
  global ma, manti, expo, expos, bina, dec, resultat;
  Digits:=70:
  if(x=0) then resultat:="0000000000000000";
  elif(x=-0) then resultat:="8000000000000000";
  elif(x=2) then resultat:="4000000000000000";
  elif(x=-2) then resultat:="C000000000000000";
  else
  ma:=IEEEdouble(x):
  expo:=ma[1]:
  if (ma[2]&lt;0) then unite:=-1; else unite:=1; fi:
  manti:=unite*round(((ma[3]*10**(-expo))-unite)*10**52):
  expos:=convert((expo+1023), binary)*10**52:
    
   if (ma[2]&lt;0) then 
     bina:=expos+manti+10**63:
   else
     bina:=expos+manti:
   end if:  
   dec:=convert(bina, decimal, binary):
   resultat:=convert(dec, hex);
  end if;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle39" style="_pstyle39"/><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1">Renvoie l'\351criture hexadecimale d'un double IEEE, en separant la partie haute de la partie basse. </Font></Text-field><Text-field layout="_pstyle39" style="_pstyle39"/></Input></Group><Group><Input><Text-field layout="_pstyle35" style="Text"><Font encoding="ISO8859-1"> Moulinette inverse de Hexa2ieee, en entr\351e, on prend un hexad\351cimal sur 16 chiffres, \351crit entre crochets et on renvoie un double IEEE.</Font></Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">ieee2Hexa:= proc(x)
  local signe, hex1, hex2, ma, manti, expo, expos, bina, bin1, bin2, dec1, dec2;
  if(x=0) then resultat:=["00000000","00000000"];
  elif(x=-0) then resultat:=["80000000","00000000"];
  elif(x=2) then resultat:=["40000000","00000000"];
  elif(x=-2) then resultat:=["C0000000","00000000"];
  else
   ma:=IEEEdouble(x);
   expo:=ma[1]:
   if (ma[2]&lt;0) then 
    manti:=2**64 + 2**63+(-ma[2]-1)*2**52+(expo+1023)*2**52;
   else 
     manti:=2**64 + (ma[2]-1)*2**52+(expo+1023)*2**52;
   fi:
   hex2:=convert(manti, hex); 
   hex2:=convert(hex2, string):  
  
   resultat:=[substring(hex2,2..9), substring(hex2,10..18)];
  end if;
  resultat;
end proc:</Text-field></Input><Output><Text-field layout="Warning" style="Warning">Warning, `resultat` is implicitly declared local to procedure `ieee2Hexa`
</Text-field></Output></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">Hexa2ieee:= proc(hexa)
local dec, bin, expobin, expo, mantis, sign, hex1, hex2, hexcat;
global res;

hex1:= op(1, hexa):
hex2:= op(2, hexa):
hexcat:= cat(hex1, hex2);
dec:= convert(hexcat, decimal, hex):

if(dec &gt;= 2**63) then
  dec = dec - 2**63:
  sign:= -1:
else
  sign:= 1:
fi;  
expo:= trunc(dec/(2**52))-1023:
mantis:= 1+frac(dec/(2**52));
res:= evalf(sign*2**(expo)*mantis);
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">ieee2Hexa(Hexa2ieee(["00124FDE","32100DFD"]));</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM3JFEpMDAxMjRGREU2IlEpMzIxMDBERkRGJQ==</Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">[substring("123",1..2)];</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM3I1EjMTI2Ig==</Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle39" style="_pstyle39"/><Text-field layout="_pstyle35" style="Text">Retourne l'ecriture hexadecimale d'un nombre ecrit sur deux double IEEE. </Text-field><Text-field layout="_pstyle39" style="_pstyle39"/></Input></Group><Group><Input><Text-field layout="_pstyle36" prompt="&gt; " style="_cstyle10">H2:=proc(res)
